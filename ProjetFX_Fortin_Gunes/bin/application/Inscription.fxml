   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #ffcdd2;" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="champIdentifiant" layoutX="53.0" layoutY="99.0" />
            <TextField fx:id="champMotDePasse" layoutX="51.0" layoutY="175.0" />
            <Button fx:id="confirmer" layoutX="59.0" layoutY="326.0" mnemonicParsing="false" onAction="#clicConfirmer" text="Confirmer et continuer" />
